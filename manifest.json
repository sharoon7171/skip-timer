{
  "manifest_version": 3,
  "name": "Link Redirect Bypass",
  "version": "1.0.0",
  "description": "Automatically bypass intermediate link services and redirect to final download pages",
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "webRequest",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-scripts/main-content-script.js"]
    }
  ]
}
